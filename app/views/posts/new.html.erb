<% flash.each do |type, msg| %>
    <div>
    <%= msg %>
    </div>
<% end %>
<h1>Add Post</h1>
<%= form_with url: user_posts_path, method: :post, local: true, scope: :post do |f| %>
    <p>
        <%= f.label :title %><br>
        <%= f.text_field :title %>
    </p>
    <p>
        <%= f.label :text %><br>
        <%= f.text_field :text %>
    </p>
    <p>
        <%= f.submit %>
    </p>
<%end%>